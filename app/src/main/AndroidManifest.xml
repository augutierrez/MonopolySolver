<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.monopolysolver">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/monopoly_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FillPlayers"/>
        <activity android:name=".FillBalance"/>
        <activity android:name=".FillProperties"/>
        <activity android:name=".EditProperty"/>
        <activity android:name=".EditBalance"/>
        <activity android:name=".ComputeWinner"/>
    </application>

</manifest>